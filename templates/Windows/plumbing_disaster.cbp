<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{soup_app_name}" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug">
				<Option output="bin/debug/{soup_app_name}" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					{soup_include_paths}
				</Compiler>
				<Linker>
					<Add option="-static-libstdc++" />
					<Add option="-static-libgcc" />
                    <Add option="-lmingw32" />
                    <Add option="-lSDL2main" />
					<Add option="-lSDL2" />
					<Add option="-lSDL2_mixer" />
					<Add option="-lSDL2_ttf" />
					{soup_lib_paths}
					<Add option="-Wl,-Bstatic -lstdc++ -lpthread -Wl,-Bdynamic -static-libgcc -static-libstdc++" />
				</Linker>
			</Target>
			<Target title="release">
				<Option output="bin/release/{soup_app_name}" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					{soup_include_paths}
				</Compiler>
				<Linker>
					<Add option="-static-libstdc++" />
					<Add option="-static-libgcc" />
                    <Add option="-lmingw32" />
                    <Add option="-lSDL2main" />
					<Add option="-lSDL2" />
					<Add option="-lSDL2_mixer" />
					<Add option="-lSDL2_ttf" />
					{soup_lib_paths}
					<Add option="-Wl,-Bstatic -lstdc++ -lpthread -Wl,-Bdynamic -static-libgcc -static-libstdc++" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add option="-std=c++1z" />
		</Compiler>
        {soup_header_paths}
        {soup_source_paths}
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
